{
  "name": "axie-data-hub",
  "version": "1.0.0",
  "description": "API for Axie Infinity",
  "author": "Luis Marsiglia",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production nodemon index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "git": "git add . && git commit -m",
    "postgit": "git push origin main",
    "heroku": "git push heroku main",
    "glitch": "git push glitch main",
    "test": "cross-env NODE_ENV=test PORT=1234 jest",
    "test:watch": "npm run test -- --watch",
    "setvar": "heroku config:set",
    "getvar": "heroku config:get"
  },
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "6.16.1",
    "@sentry/tracing": "6.16.1",
    "axios": "0.24.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "12.0.4",
    "ethers": "5.5.3",
    "express": "4.17.2",
    "mongoose": "6.1.4",
    "morgan": "1.10.0",
    "node-cache": "5.1.2",
    "nodemon": "2.0.15"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "jest": "27.4.7",
    "supertest": "6.2.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  }

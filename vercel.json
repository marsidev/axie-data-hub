{
  "version": 2,
  "name": "axie-infinity-api",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/**",
          "node_modules/**"
        ]
      }
    },
    { "src": "src/middlewares/mongoose.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}

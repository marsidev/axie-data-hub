{
  "version": 2,
  "name": "axie-infinity-api",
  "rewrites": [
    {
      "source": "/(.*)", 
      "destination": "" 
    }
  ],
  "builds": [
    {
      "use": "@vercel/node@latest",
      "src": "api/src/controllers/(.*)",
      "config": {
        "zeroConfig": true
      }
    },
    {
      "use": "@vercel/node@latest",
      "src": "api/src/middlewares/(.*)",
      "config": {
        "zeroConfig": true
      }
    },
    {
      "use": "@vercel/node@latest",
      "src": "index.js",
      "config": {
        "zeroConfig": true
      }
    }
  ]
}
